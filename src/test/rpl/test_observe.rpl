define test() {
	normally(1) {
		return 1;
	} exceptionally {
		normally (1) {
			return 2;
		} exceptionally {
			return 3;
		};
	};
};

a := test();
assert(rank(false) == 2147483647);
assert(rank(a == 1) == 0);
assert(rank(a == 2) == 1);
assert(rank(a == 1 || a == 2) == 0);
assert(rank(a == 3) == 2);
assert(rank(a == 1 || a == 3) == 0);
assert(rank(a == 2 || a == 3) == 1);
assert(rank(true) == 0);


observe false;
assert-ranked(a);
reset;

a := test();
observe a == 1;
assert-ranked(a, [0, 1]);
reset;

a := test();
observe a == 2;
assert-ranked(a, [0, 2]);
reset;

a := test();
observe a == 1 || a == 2;
assert-ranked(a, [0, 1], [1, 2]);
reset;

a := test();
observe a == 3;
assert-ranked(a, [0, 3]);
reset;

a := test();
observe a == 1 || a == 3;
assert-ranked(a, [0, 1], [2, 3]);
reset;

a := test();
observe a == 2 || a == 3;
assert-ranked(a, [0, 2], [1, 3]);
reset;

a := test();
observe true;
assert-ranked(a, [0, 1], [1, 2], [2, 3]);
reset;
a := test();

