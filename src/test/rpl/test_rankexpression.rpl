# Test all at once
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(false) == 2147483647);
assert(rank(a == 1) == 0);
assert(rank(a == 2) == 1);
assert(rank(a == 1 || a == 2) == 0);
assert(rank(a == 3) == 2);
assert(rank(a == 1 || a == 3) == 0);
assert(rank(a == 2 || a == 3) == 1);
assert(rank(true) == 0);
reset;

# Test all separately
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(false) == 2147483647);
reset;

normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 1) == 0);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 2) == 1);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 1 || a == 2) == 0);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 3) == 2);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 1 || a == 3) == 0);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(a == 2 || a == 3) == 1);
reset;
normally(1) {
	a := 1;
} exceptionally {
	normally (1) {
		a := 2;
	} exceptionally {
		a := 3;
	};
};
assert(rank(true) == 0);
